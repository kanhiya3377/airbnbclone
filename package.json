{
  "name": "airbnbclone",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node api/index.js",
     "dev-backend": "cd api && nodemon index.js",
    "dev-frontend": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.2"
  }
}
